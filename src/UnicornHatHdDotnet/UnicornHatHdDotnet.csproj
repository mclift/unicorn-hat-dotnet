<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>Mclift.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Mclift.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <Title></Title>
    <Authors>mclift</Authors>
    <Company />
    <Description>A .NET Library to drive the Pimoroni Unicorn HAT HD on the Raspberry Pi.</Description>
    <RepositoryUrl>https://github.com/mclift/unicorn-hat-dotnet</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Iot.Device.Bindings" Version="2.1.0" />
  </ItemGroup>

</Project>
